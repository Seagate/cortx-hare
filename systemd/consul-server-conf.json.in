{
  "server": true,
  "leave_on_terminate": true,
  "watches": [
    {
      "type": "keyprefix",
      "prefix": "bq/",
      "handler_type": "http",
      "http_handler_config": {
        "path": "HAX_HTTP_PROTOCOL://localhost:HAX_HTTP_PORT/watcher/bq",
        "method": "POST",
        "tls_skip_verify": true,
        "timeout": "10s"
      }
    },
    {
      "type": "service",
      "service": "hax",
      "handler_type": "http",
      "http_handler_config": {
        "path": "HAX_HTTP_PROTOCOL://localhost:HAX_HTTP_PORT",
        "method": "POST",
        "tls_skip_verify": true,
        "timeout": "10s"
      }
    },
    {
      "type": "service",
      "service": "hax",
      "args": [ "/opt/seagate/cortx/hare/libexec/consul-watch-handler",
                "--log-dir", "TMP_LOG_DIR" ]
    },
    {
      "type": "service",
      "service": "confd",
      "handler_type": "http",
      "http_handler_config": {
        "path": "HAX_HTTP_PROTOCOL://localhost:HAX_HTTP_PORT",
        "method": "POST",
        "tls_skip_verify": true,
        "timeout": "10s"
      }
    },
    {
      "type": "service",
      "service": "confd",
      "args": [ "/opt/seagate/cortx/hare/libexec/consul-watch-handler",
                "--log-dir", "TMP_LOG_DIR" ]
    },
    {
      "type": "service",
      "service": "ios",
      "handler_type": "http",
      "http_handler_config": {
        "path": "HAX_HTTP_PROTOCOL://localhost:HAX_HTTP_PORT",
        "method": "POST",
        "tls_skip_verify": true,
        "timeout": "10s"
      }
    },
    {
      "type": "service",
      "service": "ios",
      "args": [ "/opt/seagate/cortx/hare/libexec/consul-watch-handler",
                "--log-dir", "TMP_LOG_DIR" ]
    },
    {
      "type": "service",
      "service": "s3service",
      "handler_type": "http",
      "http_handler_config": {
        "path": "HAX_HTTP_PROTOCOL://localhost:HAX_HTTP_PORT",
        "method": "POST",
        "tls_skip_verify": true,
        "timeout": "10s"
      }
    },
    {
      "type": "service",
      "service": "s3service",
      "args": [ "/opt/seagate/cortx/hare/libexec/consul-watch-handler",
                "--log-dir", "TMP_LOG_DIR" ]
    }
  ],
  "enable_local_script_checks": true,
  "services": []
}
