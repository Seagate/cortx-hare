#!/usr/bin/env bash
set -eu -o pipefail
export PS4='+ [${BASH_SOURCE[0]##*/}:${LINENO}${FUNCNAME[0]:+:${FUNCNAME[0]}}] '
set -x

. ci/functions.sh  # _time, ci_init_m0vg, ci_success

echo 'XXX Hello from Jenkins!'; exit 99

cd $WORKSPACE_DIR

ci_init_m0vg
_time $M0VG run /data/hare/ci/m0vg/init-node
_time $M0VG run /data/hare/ci/m0vg/test-boot1
XXX_with_s3server && _time $M0VG run /data/hare/ci/m0vg/test-boot1-s3

ci_success
